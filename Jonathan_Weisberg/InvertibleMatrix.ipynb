   "source": [
    "import numpy as np\n",
    "from numpy import linalg"
   ]
  },
  {
   "attachments": {},
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "##### A square matrix"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "(3, 3)\n",
      "[[1 2 3]\n",
      " [0 1 4]\n",
      " [5 6 0]]\n",
      "\n",
      "[[-24.  18.   5.]\n",
      " [ 20. -15.  -4.]\n",
      " [ -5.   4.   1.]]\n",
      "\n",
      "[[ 1. -0.  0.]\n",
      " [ 0.  1.  0.]\n",
      " [ 0. -0.  1.]]\n"
     ]
    }
   ],
   "source": [
    "A = np.array([[1, 2, 3],[0, 1, 4],[5, 6, 0]])\n",
    "\n",
    "print(A)\n",
    "print()\n",
    "\n",
    "A_inv = linalg.inv(A)\n",
    "print(A_inv)\n",
    "print()\n",
    "\n",
    "AA_inv = np.round(np.matmul(A, A_inv))\n",
    "print(AA_inv)"
   ]
  },
